{
    "name": "qnn",
    "displayName": "qnn",
    "description": "qnn-table、qnn-form智能助手",
    "version": "1.1.3",
    "engines": {
        "vscode": "^1.39.0"
    },
    "repository": "https://github.com/wangzongming/qnn",
    "publisher": "xm",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*",
        "onLanguage:javascript",
        "onCommand:qnnDoc.start"
    ],
    "main": "./extension.js",
    "icon": "q.png",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.qnn",
                "title": "hello"
            },
            {
                "command": "tree.open",
                "title": "Open"
            },
            {
                "command": "qnnDoc.start",
                "title": "qnn document",
                "category": "QnnForm"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "qnnReadme",
                    "title": "Qnn文档",
                    "icon": "q.png"
                }
            ]
        },
        "views": {
            "qnnReadme": [
                {
                    "id": "qnnReadmepages",
                    "name": "各组件文档"
                }
            ]
        },
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/qnnForm.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/qnnTable.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "",
            "properties": {
                "qnn.disabled": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "是否启用qnn插件"
                }
            }
        }
    },
    "scripts": {
        "test": "node ./test/runTest.js",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.39.0",
        "eslint": "^5.13.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "vscode-test": "^1.2.0"
    },
    "dependencies": {
        "highlight.js": "^10.1.2",
        "markdown-it": "^10.0.0"
    }
}
