    "QnnForm页面代码结构生成": {
        "prefix": [
            "rq",
            "rqf",
            "reactQnnForm"
        ],
        "body": [
            "import React,{ Component } from 'react';",
            "import QnnForm from 'apih5/modules/qnn-table/qnn-form';",
            "class Page extends Component {",
            "\trender() {",
            "\t\treturn (",
            "\t\t\t<div style={{height:'100%'}}>",
            "\t\t\t\t<QnnForm",
            "\t\t\t\t\tfetch={this.props.myFetch}",
            "\t\t\t\t\tupload={this.props.myUpload}",
            "\t\t\t\t\theaders={{token: this.props.loginAndLogoutInfo.loginInfo.token}}",
            "\t\t\t\t\twrappedComponentRef={(me) => {this.qnnForm = me;this.form=me.form}}",
            "\t\t\t\t\tmethod={{ ${1}  }}",
            "\t\t\t\t\tcomponentsKey={{}}",
            "\t\t\t\t\tformConfig={[ ${2} ]}",
            "\t\t\t\t/>",
            "\t\t</div>)",
            "\t}",
            "}",
            "export default Page;"
        ],
        "description": "qnnForm QnnForm页面代码结构生成"
    },
